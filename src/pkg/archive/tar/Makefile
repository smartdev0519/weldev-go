# DO NOT EDIT.  Automatically generated by gobuild.
# gobuild -m >Makefile

D=/archive/

include $(GOROOT)/src/Make.$(GOARCH)
AR=gopack

default: packages

clean:
	rm -rf *.[$(OS)] *.a [$(OS)].out _obj

test: packages
	gotest

coverage: packages
	gotest
	6cov -g $$(pwd) | grep -v '_test\.go:'

%.$O: %.go
	$(GC) -I_obj $*.go

%.$O: %.c
	$(CC) $*.c

%.$O: %.s
	$(AS) $*.s

O1=\
	common.$O\

O2=\
	untar.$O\
	writer.$O\


phases: a1 a2
_obj$D/tar.a: phases

a1: $(O1)
	$(AR) grc _obj$D/tar.a common.$O
	rm -f $(O1)

a2: $(O2)
	$(AR) grc _obj$D/tar.a untar.$O writer.$O
	rm -f $(O2)


newpkg: clean
	mkdir -p _obj$D
	$(AR) grc _obj$D/tar.a

$(O1): newpkg
$(O2): a1
$(O3): a2

nuke: clean
	rm -f $(GOROOT)/pkg/$(GOOS)_$(GOARCH)$D/tar.a

packages: _obj$D/tar.a

install: packages
	test -d $(GOROOT)/pkg && mkdir -p $(GOROOT)/pkg/$(GOOS)_$(GOARCH)$D
	cp _obj$D/tar.a $(GOROOT)/pkg/$(GOOS)_$(GOARCH)$D/tar.a
