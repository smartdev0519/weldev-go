# DO NOT EDIT.  Automatically generated by gobuild.
# gobuild -m >Makefile
O=6
GC=$(O)g
CC=$(O)c -w
AS=$(O)a
AR=$(O)ar

default: packages

clean:
	rm -f *.$O *.a $O.out

test: packages
	gotest

coverage: packages
	gotest
	6cov -g `pwd` | grep -v '_test\.go:'

%.$O: %.go
	$(GC) $*.go

%.$O: %.c
	$(CC) $*.c

%.$O: %.s
	$(AS) $*.s

O1=\
	letter.$O\

unicode.a: a1

a1:	$(O1)
	$(AR) grc unicode.a letter.$O
	rm -f $(O1)

newpkg: clean
	$(AR) grc unicode.a

$(O1): newpkg

nuke: clean
	rm -f $(GOROOT)/pkg/unicode.a

packages: unicode.a

install: packages
	cp unicode.a $(GOROOT)/pkg/unicode.a

