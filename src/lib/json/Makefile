# DO NOT EDIT.  Automatically generated by gobuild.
# gobuild -m >Makefile

O_arm=5
O_amd64=6
O_386=8
OS=568vq

O=$(O_$(GOARCH))
GC=$(O)g
CC=$(O)c -FVw
AS=$(O)a
AR=6ar

default: packages

clean:
	rm -f *.[$(OS)] *.a [$(OS)].out

test: packages
	gotest

coverage: packages
	gotest
	6cov -g `pwd` | grep -v '_test\.go:'

%.$O: %.go
	$(GC) $*.go

%.$O: %.c
	$(CC) $*.c

%.$O: %.s
	$(AS) $*.s

O1=\
	parse.$O\

O2=\
	generic.$O\
	struct.$O\


phases: a1 a2
json.a: phases

a1: $(O1)
	$(AR) grc json.a parse.$O
	rm -f $(O1)

a2: $(O2)
	$(AR) grc json.a generic.$O struct.$O
	rm -f $(O2)


newpkg: clean
	$(AR) grc json.a

$(O1): newpkg
$(O2): a1
$(O3): a2

nuke: clean
	rm -f $(GOROOT)/pkg/json.a

packages: json.a

install: packages
	cp json.a $(GOROOT)/pkg/json.a
