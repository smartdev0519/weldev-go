# DO NOT EDIT.  Automatically generated by gobuild.
# gobuild -m >Makefile
O=6
GC=$(O)g
CC=$(O)c -w
AS=$(O)a
AR=$(O)ar

default: packages

clean:
	rm -f *.$O *.a $O.out

test: packages
	gotest

%.$O: %.go
	$(GC) $*.go

%.$O: %.c
	$(CC) $*.c

%.$O: %.s
	$(AS) $*.s

O1=\
	url.$O\

O2=\
	request.$O\

O3=\
	conn.$O\

O4=\
	server.$O\

O5=\
	triv.$O\

http.a: a1 a2 a3 a4 a5
main.a: a1 a2 a3 a4 a5

a1:	$(O1)
	$(AR) grc http.a url.$O
	rm -f $(O1)

a2:	$(O2)
	$(AR) grc http.a request.$O
	rm -f $(O2)

a3:	$(O3)
	$(AR) grc http.a conn.$O
	rm -f $(O3)

a4:	$(O4)
	$(AR) grc http.a server.$O
	rm -f $(O4)

a5:	$(O5)
	$(AR) grc main.a triv.$O
	rm -f $(O5)

newpkg: clean
	$(AR) grc http.a
	$(AR) grc main.a

$(O1): newpkg
$(O2): a1
$(O3): a2
$(O4): a3
$(O5): a4

nuke: clean
	rm -f $(GOROOT)/pkg/http.a $(GOROOT)/pkg/main.a

packages: http.a main.a

install: packages
	cp http.a $(GOROOT)/pkg/http.a
	cp main.a $(GOROOT)/pkg/main.a

