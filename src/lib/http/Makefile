# DO NOT EDIT.  Automatically generated by gobuild.
# gobuild -m >Makefile

D=

include $(GOROOT)/src/Make.$(GOARCH)
AR=gopack

default: packages

clean:
	rm -rf *.[$(OS)] *.a [$(OS)].out _obj

test: packages
	gotest

coverage: packages
	gotest
	6cov -g `pwd` | grep -v '_test\.go:'

%.$O: %.go
	$(GC) -I_obj $*.go

%.$O: %.c
	$(CC) $*.c

%.$O: %.s
	$(AS) $*.s

O1=\
	status.$O\
	url.$O\

O2=\
	request.$O\

O3=\
	server.$O\

O4=\
	fs.$O\


phases: a1 a2 a3 a4
_obj$D/http.a: phases

a1: $(O1)
	$(AR) grc _obj$D/http.a status.$O url.$O
	rm -f $(O1)

a2: $(O2)
	$(AR) grc _obj$D/http.a request.$O
	rm -f $(O2)

a3: $(O3)
	$(AR) grc _obj$D/http.a server.$O
	rm -f $(O3)

a4: $(O4)
	$(AR) grc _obj$D/http.a fs.$O
	rm -f $(O4)


newpkg: clean
	mkdir -p _obj$D
	$(AR) grc _obj$D/http.a

$(O1): newpkg
$(O2): a1
$(O3): a2
$(O4): a3
$(O5): a4

nuke: clean
	rm -f $(GOROOT)/pkg/$(GOOS)_$(GOARCH)$D/http.a

packages: _obj$D/http.a

install: packages
	test -d $(GOROOT)/pkg && mkdir -p $(GOROOT)/pkg/$(GOOS)_$(GOARCH)$D
	cp _obj$D/http.a $(GOROOT)/pkg/$(GOOS)_$(GOARCH)$D/http.a
