# DO NOT EDIT.  Automatically generated by gobuild.
# gobuild -m >Makefile
O=6
GC=$(O)g
CC=$(O)c -w
AS=$(O)a
AR=$(O)ar

default: packages

clean:
	rm -f *.$O *.a $O.out

test: packages
	gotest

coverage: packages
	gotest
	6cov -g `pwd` | grep -v '_test\.go:'

%.$O: %.go
	$(GC) $*.go

%.$O: %.c
	$(CC) $*.c

%.$O: %.s
	$(AS) $*.s

O1=\
	bytebuffer.$O\
	io.$O\

O2=\
	pipe.$O\

io.a: a1 a2

a1:	$(O1)
	$(AR) grc io.a bytebuffer.$O io.$O
	rm -f $(O1)

a2:	$(O2)
	$(AR) grc io.a pipe.$O
	rm -f $(O2)

newpkg: clean
	$(AR) grc io.a

$(O1): newpkg
$(O2): a1

nuke: clean
	rm -f $(GOROOT)/pkg/io.a

packages: io.a

install: packages
	cp io.a $(GOROOT)/pkg/io.a

