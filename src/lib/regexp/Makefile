# DO NOT EDIT.  Automatically generated by gobuild.
# gobuild -m >Makefile

O_arm=5
O_amd64=6
O_386=8
OS=568vq

O=$(O_$(GOARCH))
GC=$(O)g
CC=$(O)c -FVw
AS=$(O)a
AR=6ar

default: packages

clean:
	rm -f *.[$(OS)] *.a [$(OS)].out

test: packages
	gotest

coverage: packages
	gotest
	6cov -g `pwd` | grep -v '_test\.go:'

%.$O: %.go
	$(GC) $*.go

%.$O: %.c
	$(CC) $*.c

%.$O: %.s
	$(AS) $*.s

O1=\
	regexp.$O\


phases: a1
regexp.a: phases

a1: $(O1)
	$(AR) grc regexp.a regexp.$O
	rm -f $(O1)


newpkg: clean
	$(AR) grc regexp.a

$(O1): newpkg
$(O2): a1

nuke: clean
	rm -f $(GOROOT)/pkg/regexp.a

packages: regexp.a

install: packages
	cp regexp.a $(GOROOT)/pkg/regexp.a
