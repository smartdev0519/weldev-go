#    os_types.go stat_amd64_linux.go >Makefile
O=6
GC=$(O)g
CC=$(O)c -w
AS=$(O)a
AR=$(O)ar

default: packages

clean:
	rm -f *.$O *.a $O.out

test: packages
	gotest

coverage: packages
	gotest
	6cov -g `pwd` | grep -v '_test\.go:'

%.$O: %.go
	$(GC) $*.go

%.$O: %.c
	$(CC) $*.c

%.$O: %.s
	$(AS) $*.s

O1=\
	os_error.$O\
	os_types.$O\

O2=\
	os_env.$O\
	os_time.$O\
	stat_$(GOARCH)_$(GOOS).$O\

O3=\
	os_file.$O\

os.a: a1 a2 a3

a1:	$(O1)
	$(AR) grc os.a os_error.$O os_types.$O
	rm -f $(O1)

a2:	$(O2)
	$(AR) grc os.a os_env.$O os_time.$O stat_$(GOARCH)_$(GOOS).$O
	rm -f $(O2)

a3:	$(O3)
	$(AR) grc os.a os_file.$O
	rm -f $(O3)

newpkg: clean
	$(AR) grc os.a

$(O1): newpkg
$(O2): a1
$(O3): a2

nuke: clean
	rm -f $(GOROOT)/pkg/os.a

packages: os.a

install: packages
	cp os.a $(GOROOT)/pkg/os.a

