O=6
GC=$(O)g
CC=$(O)c -w
AS=$(O)a
AR=$(O)ar

default: packages

clean:
	rm -f *.$O *.a $O.out

test: packages
	gotest

coverage: packages
	gotest
	6cov -g `pwd` | grep -v '_test\.go:'

%.$O: %.go
	$(GC) $*.go

%.$O: %.c
	$(CC) $*.c

%.$O: %.s
	$(AS) $*.s

O1=\
	fd_$(GOOS).$O\
	ip.$O\
	dnsmsg.$O\

O2=\
	fd.$O\
	net_$(GOOS).$O\

O3=\
	net.$O\

net.a: a1 a2 a3

a1:	$(O1)
	$(AR) grc net.a fd_$(GOOS).$O ip.$O dnsmsg.$O
	rm -f $(O1)

a2:	$(O2)
	$(AR) grc net.a fd.$O net_$(GOOS).$O
	rm -f $(O2)

a3:	$(O3)
	$(AR) grc net.a net.$O
	rm -f $(O3)

newpkg: clean
	$(AR) grc net.a

$(O1): newpkg
$(O2): a1
$(O3): a2

nuke: clean
	rm -f $(GOROOT)/pkg/net.a

packages: net.a

install: packages
	cp net.a $(GOROOT)/pkg/net.a

