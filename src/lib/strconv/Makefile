# DO NOT EDIT.  Automatically generated by gobuild.
# gobuild -m >Makefile

O_arm=5
O_amd64=6
O_386=8
OS=568vq

O=$(O_$(GOARCH))
GC=$(O)g
CC=$(O)c -FVw
AS=$(O)a
AR=6ar

default: packages

clean:
	rm -f *.[$(OS)] *.a [$(OS)].out

test: packages
	gotest

coverage: packages
	gotest
	6cov -g `pwd` | grep -v '_test\.go:'

%.$O: %.go
	$(GC) $*.go

%.$O: %.c
	$(CC) $*.c

%.$O: %.s
	$(AS) $*.s

O1=\
	atoi.$O\
	decimal.$O\
	itoa.$O\
	quote.$O\

O2=\
	ftoa.$O\

O3=\
	atof.$O\


phases: a1 a2 a3
strconv.a: phases

a1: $(O1)
	$(AR) grc strconv.a atoi.$O decimal.$O itoa.$O quote.$O
	rm -f $(O1)

a2: $(O2)
	$(AR) grc strconv.a ftoa.$O
	rm -f $(O2)

a3: $(O3)
	$(AR) grc strconv.a atof.$O
	rm -f $(O3)


newpkg: clean
	$(AR) grc strconv.a

$(O1): newpkg
$(O2): a1
$(O3): a2
$(O4): a3

nuke: clean
	rm -f $(GOROOT)/pkg/strconv.a

packages: strconv.a

install: packages
	cp strconv.a $(GOROOT)/pkg/strconv.a
