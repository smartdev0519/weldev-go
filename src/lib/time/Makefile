# DO NOT EDIT.  Automatically generated by gobuild.
# gobuild -m >Makefile
O=6
GC=$(O)g
CC=$(O)c -w
AS=$(O)a
AR=$(O)ar

default: packages

clean:
	rm -f *.$O *.a $O.out

test: packages
	gotest

coverage: packages
	gotest
	6cov -g `pwd` | grep -v '_test\.go:'

%.$O: %.go
	$(GC) $*.go

%.$O: %.c
	$(CC) $*.c

%.$O: %.s
	$(AS) $*.s

O1=\
	zoneinfo.$O\

O2=\
	time.$O\

O3=\
	tick.$O\

time.a: a1 a2 a3

a1:	$(O1)
	$(AR) grc time.a zoneinfo.$O
	rm -f $(O1)

a2:	$(O2)
	$(AR) grc time.a time.$O
	rm -f $(O2)

a3:	$(O3)
	$(AR) grc time.a tick.$O
	rm -f $(O3)

newpkg: clean
	$(AR) grc time.a

$(O1): newpkg
$(O2): a1
$(O3): a2

nuke: clean
	rm -f $(GOROOT)/pkg/time.a

packages: time.a

install: packages
	cp time.a $(GOROOT)/pkg/time.a

